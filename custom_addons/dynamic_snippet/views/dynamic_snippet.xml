<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="product_list_template" name="Dynamic Snippets">
        <section class="three_product_dynamic_snippet o_snippet">
            <h3 class="text-center">Product List</h3>
            <div class="row">
                <t t-foreach="data" t-as="product">
                    <div class="p-3 col-md-4">
                        <div class="card" style="width: 16rem;">
                            <div class="card-body">
                                <h5 class="card-title text-center" t-esc="product['name']"/>
                            </div>
                            <img class="card-img-top" t-att-src="product['image_url']" alt="Product Image" />

                        </div>
                    </div>
                </t>
            </div>
        </section>
    </template>
</odoo>
